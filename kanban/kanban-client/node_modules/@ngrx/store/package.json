{
  "name": "@ngrx/store",
  "version": "18.0.0",
  "description": "RxJS powered Redux for Angular apps",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngrx/platform.git"
  },
  "keywords": [
    "RxJS",
    "Angular",
    "Redux",
    "NgRx",
    "Schematics",
    "Angular CLI"
  ],
  "author": "NgRx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngrx/platform/issues"
  },
  "homepage": "https://github.com/ngrx/platform#readme",
  "peerDependencies": {
    "@angular/core": "^18.0.0",
    "rxjs": "^6.5.3 || ^7.5.0"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "packageGroup": [
      "@ngrx/store",
      "@ngrx/effects",
      "@ngrx/entity",
      "@ngrx/router-store",
      "@ngrx/data",
      "@ngrx/schematics",
      "@ngrx/store-devtools",
      "@ngrx/component-store",
      "@ngrx/component",
      "@ngrx/eslint-plugin",
      "@ngrx/signals"
    ],
    "migrations": "./migrations/migration.json"
  },
  "sideEffects": false,
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "module": "fesm2022/ngrx-store.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/ngrx-store.mjs",
      "esm": "./esm2022/ngrx-store.mjs",
      "default": "./fesm2022/ngrx-store.mjs"
    },
    "./testing": {
      "types": "./testing/index.d.ts",
      "esm2022": "./esm2022/testing/ngrx-store-testing.mjs",
      "esm": "./esm2022/testing/ngrx-store-testing.mjs",
      "default": "./fesm2022/ngrx-store-testing.mjs"
    }
  }
}