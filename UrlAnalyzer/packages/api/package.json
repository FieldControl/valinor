{
	"name": "url_analyzer_api",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"start:dev": "yarn build && yarn dev",
		"build:copy": "cpy ./src/types/*.ts ../client/src/types",
		"build:clean": "del-cli dist",
		"build:tsc": "tsc --project ./tsconfig.json",
		"build:alias": "tsc-alias -p ./tsconfig.json",
		"build": "yarn build:copy && yarn build:clean && yarn build:tsc && yarn build:alias",
		"lint": "prettier --check . && eslint src --ext ts",
		"format": "prettier --write . && eslint src --ext ts --fix",
		"dev": "env-cmd node --enable-source-maps dist/index.js",
		"start": "next start",
		"migrate": "env-cmd ley"
	},
	"main": "dist/index.js",
	"author": "JPBM135 <jpedrobm0@gmail.com>",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.16",
		"@types/micromatch": "^4.0.2",
		"@types/node": "^18.11.18",
		"cpy-cli": "^4.2.0",
		"del-cli": "^5.0.0",
		"discord-api-types": "^0.37.31",
		"env-cmd": "^10.1.0",
		"ley": "^0.8.1",
		"tsc-alias": "^1.8.2",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@sapphire/snowflake": "^3.4.0",
		"bcrypt": "^5.1.0",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"googleapis": "^110.0.0",
		"got": "^12.5.3",
		"helmet": "^6.0.1",
		"imgur": "^2.2.0",
		"ioredis": "^5.3.0",
		"kleur": "^4.1.5",
		"lighthouse": "^10.0.0",
		"metadata-scraper": "^0.2.61",
		"micromatch": "^4.0.5",
		"postgres": "^3.3.3",
		"prom-client": "^14.1.1",
		"puppeteer": "^19.6.3",
		"raw-body": "^2.5.1",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.7.0",
		"undici": "^5.16.0"
	}
}
