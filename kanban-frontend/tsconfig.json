{
  "exclude": ["./coverage", ".dist", "__tests__", "jest.config.js"],
  "ts-node": {
    "transpileonly": true,
    "files": true
  },
 "compilerOptions": {
 "target":"ES2022",
 "module":"ES2022",
 "rootDir":"./src",
 "esModuleInterop": true,
 "checkJs": true,
 "outDir": "./dist",
 "moduleResolution": "node",
 "forceConsistentCasingInFileNames": true,
 "noImplicitAny": true,
 "strict":true,
 "skipLibCheck": true
}
}





