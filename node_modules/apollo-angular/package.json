{
  "name": "apollo-angular",
  "description": "Use your GraphQL data in your Angular app, with the Apollo Client",
  "version": "4.2.1",
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela/"
  },
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "module": "fesm2015/ngApollo.mjs",
  "typings": "index.d.ts",
  "homepage": "https://www.apollo-angular.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/kamilkisiela/apollo-angular",
    "directory": "packages/apollo-angular"
  },
  "keywords": [
    "apollo",
    "graphql",
    "angular",
    "schematics",
    "angular-schematics"
  ],
  "peerDependencies": {
    "@angular/core": "^14.0.0 || ^15.0.0",
    "@apollo/client": "^3.0.0",
    "graphql": "^15.0.0 || ^16.0.0",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "publishConfig": {
    "directory": "build",
    "access": "public"
  },
  "schematics": "./schematics/collection.json",
  "ng-update": {
    "requirements": {},
    "migrations": "./schematics/migrations.json",
    "packageGroupName": "apollo-angular",
    "packageGroup": [
      "apollo-angular",
      "@apollo/client",
      "graphql"
    ]
  },
  "es2020": "fesm2020/ngApollo.mjs",
  "esm2020": "esm2020/ngApollo.mjs",
  "fesm2020": "fesm2020/ngApollo.mjs",
  "fesm2015": "fesm2015/ngApollo.mjs",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/ngApollo.mjs",
      "es2020": "./fesm2020/ngApollo.mjs",
      "es2015": "./fesm2015/ngApollo.mjs",
      "node": "./fesm2015/ngApollo.mjs",
      "default": "./fesm2020/ngApollo.mjs"
    },
    "./headers": {
      "types": "./headers/index.d.ts",
      "esm2020": "./esm2020/headers/ngApolloLinkHeaders.mjs",
      "es2020": "./fesm2020/ngApolloLinkHeaders.mjs",
      "es2015": "./fesm2015/ngApolloLinkHeaders.mjs",
      "node": "./fesm2015/ngApolloLinkHeaders.mjs",
      "default": "./fesm2020/ngApolloLinkHeaders.mjs"
    },
    "./http": {
      "types": "./http/index.d.ts",
      "esm2020": "./esm2020/http/ngApolloLinkHttp.mjs",
      "es2020": "./fesm2020/ngApolloLinkHttp.mjs",
      "es2015": "./fesm2015/ngApolloLinkHttp.mjs",
      "node": "./fesm2015/ngApolloLinkHttp.mjs",
      "default": "./fesm2020/ngApolloLinkHttp.mjs"
    },
    "./persisted-queries": {
      "types": "./persisted-queries/index.d.ts",
      "esm2020": "./esm2020/persisted-queries/ngApolloLinkPersisted.mjs",
      "es2020": "./fesm2020/ngApolloLinkPersisted.mjs",
      "es2015": "./fesm2015/ngApolloLinkPersisted.mjs",
      "node": "./fesm2015/ngApolloLinkPersisted.mjs",
      "default": "./fesm2020/ngApolloLinkPersisted.mjs"
    },
    "./testing": {
      "types": "./testing/index.d.ts",
      "esm2020": "./esm2020/testing/ngApolloTesting.mjs",
      "es2020": "./fesm2020/ngApolloTesting.mjs",
      "es2015": "./fesm2015/ngApolloTesting.mjs",
      "node": "./fesm2015/ngApolloTesting.mjs",
      "default": "./fesm2020/ngApolloTesting.mjs"
    }
  }
}